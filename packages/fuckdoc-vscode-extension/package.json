{
  "name": "fuckdoc-vscode-extension",
  "displayName": "fuckdoc-vscode-extension",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fuckdoc.addPic",
        "title": "fuckdoc: 为该组件添加图片"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "fuckdoc.addPic",
          "group": "group1"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "tsup",
    "watch": "tsup --watch",
    "package": "tsup",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "tsup": "5.11.11",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "sharp": "^0.30.4"
  }
}
